{
  "manifest_version": 2,

  "name": "Hummingbird Context Search",
  "description": "Context search for Hummingbird",
  "version": "1.3",
  "icons": {
    "16": "logo_16.png",
    "48": "logo_48.png",
    "128": "logo_128.png"
  },

  "options_page": "options.html",

  "permissions": [
    "storage",
    "contextMenus"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "js/content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "js/humm.js"
    ]
  }
}